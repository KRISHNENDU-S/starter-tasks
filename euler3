#used python for this
#!/bin/python3

import math 
        
t=int(input())
if(1<=t and t<=10):
    for a in range(t):
        n=int(input())
        if(10<=n and n<=pow(10,12)):
            l=int(math.sqrt(n))
            for i in range(2,l+1):
                while(n%i==0 and i<n):
                    n=n//i
            print(n)
        
            



            
    

    


